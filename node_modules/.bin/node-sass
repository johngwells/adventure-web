#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node-sass@4.14.1/node_modules/node-sass/bin/node-sass/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node-sass@4.14.1/node_modules/node-sass/bin/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node-sass@4.14.1/node_modules/node-sass/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node-sass@4.14.1/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules:/Users/JW/Documents/Lambda/Projects/React/node_modules:/Users/JW/Documents/Lambda/Projects/node_modules:/Users/JW/Documents/Lambda/node_modules:/Users/JW/Documents/node_modules:/Users/JW/node_modules:/Users/node_modules:/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/node-sass/bin/node-sass/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/node-sass/bin/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/node-sass/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../node-sass/bin/node-sass" "$@"
else 
  exec node  "$basedir/../node-sass/bin/node-sass" "$@"
fi
