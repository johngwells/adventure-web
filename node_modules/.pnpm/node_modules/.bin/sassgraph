#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/bin/sassgraph/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/bin/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/sass-graph@2.2.5/node_modules/sass-graph/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/sass-graph@2.2.5/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules:/Users/JW/Documents/Lambda/Projects/React/node_modules:/Users/JW/Documents/Lambda/Projects/node_modules:/Users/JW/Documents/Lambda/node_modules:/Users/JW/Documents/node_modules:/Users/JW/node_modules:/Users/node_modules:/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node_modules/sass-graph/bin/sassgraph/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node_modules/sass-graph/bin/node_modules:/Users/JW/Documents/Lambda/Projects/React/adventure-web/node_modules/.pnpm/node_modules/sass-graph/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sass-graph/bin/sassgraph" "$@"
else 
  exec node  "$basedir/../sass-graph/bin/sassgraph" "$@"
fi
